{% embed '@components/containers/slick.twig' with {
    peekaboo: true,
    modifier: 'center'
  }%}
  {% block slide_content %}
      {% for card in cards %}
        {% set card = card | merge({
          lazyload_class: 'data-lazy',
          modifier: "special"
        }) %}
        {% include "@components/teasers/card.twig" with card %}
      {% endfor %}
  {% endblock %}
{% endembed %}
