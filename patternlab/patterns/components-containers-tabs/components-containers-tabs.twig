


<ul class="tabs" data-tabs id="{{ id }}">
  {% for tab in tabs %}
    <li class="tabs-title {{ tab.is_active ? 'is-active' }}"><a href="#{{ tab.id }}" aria-selected="true">{{ tab.text }}</a></li>
  {% endfor %}
</ul>

<div class="tabs-content" data-tabs-content="example-tabs">

    {% for tab in tabs %}
      <div id="{{ tab.id }}" class="tabs-panel {{tab.is_active ? 'is-active'}}">
        {{ tab.content }}
      </div>
    {% endfor %}

</div>
