
<div class="grid-container">
  {% embed 'components/containers/_slick.twig' with {
    pausable: true
  } %}
    {% block slide_content %}
      {% for slide in slides %}
          <div>
            <blockquote>
              <p>Lorem ipsum dolor sit amet</p>
              <cite>Name, Title</cite>
            </blockquote>
          </div>
        {% endfor %}
    {% endblock %}
  {% endembed %}
  {% embed 'components/containers/_slick.twig' with {
    peekaboo: true,
    modifier: 'cards'
  }%}
    {% block slide_content %}
      {% for i in range (i, 5) %}
            {% include "components/teasers/card.twig" with card %}
        {% endfor %}
    {% endblock %}
  {% endembed %}
</div>
