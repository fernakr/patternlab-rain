<div class="grid-container">
  <div class="slick-wrap">
    <div class="js-slick">
      {% for slide in slides %}
        <div>
          <blockquote>
            <p>Lorem ipsum dolor sit amet</p>
            <cite>Name, Title</cite>
          </blockquote>
        </div>
      {% endfor %}
    </div>
    <div class="slick-nav">
     <button class="js-slick-toggle slick-toggle">
      <span class="show-for-sr">Toggle Pause/Play</span>
      <span class="icon-pause active"></span>
      <span class="icon-play inactive"></span>
    </button>
    </div>

  </div>
  <div class="slick-wrap">
    <div class="js-slick--cards has-peekaboo">
      {% for i in range (i, 5) %}
          {% include "components/teasers/card.twig" with card %}
      {% endfor %}
    </div>
    <div class="slick-nav"></div>
  </div>
</div>
<div class="slick-wrap">
  <div class="js-slick--center">
      {% for i in range (i, 5) %}
          {% include "components/teasers/card.twig" with {
          "card": {
            "modifier": "",
            "image": {
              "src": "https://source.unsplash.com/PhYq704ffdA/640x360",
